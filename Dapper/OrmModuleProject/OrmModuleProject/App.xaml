<Application x:Class="OrmModuleProject.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OrmModuleProject"
             xmlns:vm="clr-namespace:OrmModuleProject.ViewModels"
             xmlns:v="clr-namespace:OrmModuleProject.Views"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:StartAppViewModel}">
            <v:StartAppView/>
        </DataTemplate>

        <DataTemplate DataType="{x:Type vm:LogInViewModel}">
            <v:LogInView/>
        </DataTemplate>

        
    </Application.Resources>
</Application>
